{

  "data_args": {
    "datasets_path": "data",
    "resolution": 32,
    "batch_size": 128
  },
  "train_args": {
    "num_epochs": 300,
    "lr": 0.001,
    "momentum": 0.9,
    "cosine_max_epochs": 20,
    "eta_min": 0.00001,
    "tau": 3.0,
    "lambda": 0.3,
    "checkpoint_interval": 10,
    "checkpoint_path": "checkpoint",
    "model_save_path": "model_save",
    "model_save_name": "vit"
  },
  "vit_args": {
    "model_name": "vit",
    "in_channels": 3,
    "patch_size": 2,
    "embed_size": 192,

    "image_size": 32,
    "depth": 12,
    "num_heads": 6,
    "drop_p": 0.3,
    "forward_expansion": 4,
    "forward_drop_p": 0.2,
    "num_classes": 10,
    "if_cls": true,
    "if_dis": false
  },
  "vit2_args": {
    "model_name": "vit2",
    "image_size": 32,
    "patch_size": 2,
    "num_classes": 10,
    "dim": 384,
    "depth": 6,
    "heads": 6,
    "mlp_dim": 768,
    "pool":"cls",
    "channels": 3,
    "dim_head": 64,
    "dropout": 0.5,
    "emb_dropout":0.5
  },
  "resnet18_1_args": {
    "model_name": "resnet18_1"
  },
  "resnet18_2_args": {
    "model_name": "resnet18_2"
  }
}